<style>
  .short-benefits-section {
    display: flex;
    flex-direction: column;
    gap: 4px;
  }

  .short-benefit {
    display: flex;
    gap: 4px;
    align-items: center;
  }

  .short-benefit svg {
    width: 28px;
    height: 28px;
    /* width: 25px;
    height: 25px; */
  }

  .trust-seals-section {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 36px;
  }

  .trust-seal {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 10px;
    text-align: center;
  }

  .trust-seal svg {
    width: 60px;
    height: 60px;
  }

  /* Social Proof Section */

  .social-proof {
    align-self: stretch;
    padding-top: 50px;
    padding-bottom: 50px;
    background: #edfaff;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    gap: 25px;
    display: flex;
  }

  .headline-social-proof {
    align-self: stretch;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    gap: 10px;
    display: flex;
    padding: 0 15px;
  }

  .headline-social-proof h1 {
    text-align: center;
  }

  .social-proof-stars {
    justify-content: center;
    align-items: center;
    flex-direction: column;
    gap: 4px;
    display: inline-flex;
  }

  .headline-stars svg {
    width: 22px !important;
    height: 22px !important;
  }

  .stars {
    justify-content: center;
    align-items: center;
    gap: 1px;
    display: flex;
  }

  .stars svg {
    width: 14px;
    height: 14px;
  }

  .clarity-star-solid {
    width: 25px;
    height: 25px;
    position: relative;
  }

  .social-proof .container {
    padding: 4px 25px;
    justify-content: center;
    align-items: center;
    gap: 20px;
    row-gap: 30px;
    display: flex;
    flex-direction: row;
  }

  .frame {
    position: relative;
    padding-bottom: 100px;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    display: inline-flex;
  }

  .social-proof img {
    width: 135px;
    height: 172px;
    object-fit: cover;
    border-radius: 12px;
    border: 5px #3ecaf2 solid;
  }

  .image-tilt-left {
    transform: rotate(-3deg);
    transform-origin: 0 0;
  }

  .image-tilt-right {
    transform: rotate(3deg);
    transform-origin: 0 0;
  }

  .review {
    position: absolute;
    z-index: 10;
    width: 220px;
    bottom: 0px;
    /* height: 122px; */
    padding: 8px 14px;
    transform-origin: 0 0;
    background: white;
    border-radius: 12px;
    overflow: hidden;
    border: 3px #068ec9 solid;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    gap: 10px;
    display: flex;
  }

  .review-text {
    font-size: 14px;
  }

  .rating-name {
    align-self: stretch;
    justify-content: flex-start;
    align-items: flex-start;
    gap: 10px;
    display: inline-flex;
  }

  .social-proof .name {
    flex: 1 1 0;
    text-align: right;
    color: #068ec9;
  }

  .item1 {
    order: 0;
  }

  .item1 .review {
    width: 230px;
    left: -25px;
    transform: rotate(1deg);
  }

  .item2 {
    order: 1;
  }

  .item3 {
    order: 2;
  }

  .item4 {
    order: 3;
  }

  .item4 .review {
    width: 250px;
    right: -25px;
    transform: rotate(-1deg);
  }

  .item5 {
    order: 4;
    padding-bottom: 75px;
  }

  .item5 .review {
    width: 270px;
    transform: rotate(1deg);
  }

  @media screen and (min-width: 495px) {
    .social-proof-stars {
      flex-direction: row;
    }

    .social-proof .container {
      padding: 4px 75px;
      gap: 45px;
    }

    .item3 {
      order: 3;
    }

    .item4 {
      order: 2;
    }

    .item4 .review {
      right: auto;
    }

    .stars svg {
      width: 18px;
      height: 18px;
    }
  }

  @media screen and (min-width: 750px) {
    .headline-social-proof h1 {
      font-size: 32px;
    }

    .social-proof-stars p {
      font-size: 16px;
    }
  }

  @media screen and (min-width: 1200px) {
    .social-proof img {
      width: 250px;
      height: 312px;
    }

    .frame {
      padding-bottom: 75px;
    }

    .frame .review {
      width: 400px;
    }

    .frame .review-text {
      font-size: 16px;
    }

    .item1 .review {
      left: auto;
    }
  }

  /* Steps Section */

  .steps {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 35px;
    padding: 50px 10px;
  }

  .steps h3 {
    font-size: 22px;
    margin: 0;
  }

  .steps .container {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 50px;
    padding: 12px;
  }

  .step-box {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    gap: 25px;
  }

  .step-box img {
    width: 200px;
    height: 200px;
    object-fit: cover;
    border-radius: 12px;
  }

  .step-box .text-box {
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    gap: 8px;
  }

  @media screen and (min-width: 750px) {
    .steps h1 {
      font-size: 32px;
    }
  }

  @media screen and (min-width: 800px) {
    .steps .container {
      gap: 75px;
    }
  }

  @media screen and (min-width: 1200px) {
    .steps h1 {
      font-size: 32px;
    }

    .steps h3 {
      font-size: 28px;
    }
  }

  /* Benefits Section */

  .benefits-section {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 50px;
    padding: 50px 15px;
  }

  .benefit-container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }

  .benefit-container img {
    order: 0;
    width: 100%;
    max-width: 440px;
    height: auto;
    background-color: gray;
    border-radius: 12px;
  }

  .benefit-container .text-box {
    order: 1;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: 25px 0;
  }

  .benefit-container .text {
    width: 100%;
    max-width: 420px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    gap: 16px;
  }

  .benefit-container h3 {
    font-size: 24px;
    font-weight: 600;
  }

  @media screen and (min-width: 1000px) {
    .benefits-section {
      padding: 75px 15px;
    }

    .benefit-container {
      flex-direction: row;
      width: 100%;
      max-width: 1200px;
    }

    .benefit-container img {
      order: 0;
    }

    .benefit-container .text-box {
      order: 0;
      padding: 50px 75px;
    }

    .benefit-container .text {
      max-width: 550px;
      gap: 22px;
    }

    .align-text-right {
      align-items: flex-end !important;
    }

    .align-text-left {
      align-items: flex-start !important;
    }

    .benefit-container h3 {
      font-size: 36px;
    }
  }

  /* Features Section */

  .features-section {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 35px;
    padding-top: 50px;
    padding-bottom: 25px;
    background-color: #3ecaf2;
  }

  .features-container {
    width: 100%;
    max-width: 450px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 35px;
    padding: 0 15px;
  }

  .features-container h1 {
    font-size: 32px;
    color: white !important;
  }

  .features-container .feature {
    display: flex;
    flex-direction: column;
    gap: 10px;
  }

  .features-container .feature h3 {
    font-size: 24px;
    font-weight: 600;
    color: white !important;
  }

  .features-container .feature p {
    font-size: 16px;
    color: white !important;
  }

  .features-section img {
    width: 100%;
    height: auto;
  }

  @media screen and (min-width: 700px) {
    .features-section {
      flex-direction: row;
      justify-content: space-between;
      gap: 15px;
    }

    .features-section img {
      width: 300px;
      height: 300px;
    }

    .features-container {
      padding-left: 25px;
      max-width: 100%;
    }

    .features-container h1 {
      margin-bottom: 20px;
    }

    .features-container .feature {
      max-width: 790px;
    }
  }

  @media screen and (min-width: 1000px) {
    .features-section img {
      width: 600px;
      height: 600px;
    }

    .features-container .feature h3 {
      font-size: 26px;
    }

    .features-container .feature p {
      font-size: 20px;
    }
  }

  /* Comparison Section */

  .comparison-section {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 65px;
    padding: 50px 15px;
  }

  .comparison-section h1 {
    width: 100%;
    text-align: center;
    /* font-size: 26px; */
  }

  @media screen and (min-width: 750px) {
    .comparison-section {
      padding: 75px 15px;
    }

    .comparison-section h1 {
      font-size: 32px;
    }

    .rmq-d0a8ba2b {
      font-size: 18px !important;
    }

    .rmq-196f5390 {
      font-size: 18px !important;
    }

    .rmq-44279447 {
      font-size: 18px !important;
    }

    .rmq-49774d36 {
      width: 50px !important;
      height: 50px !important;
    }
  }
</style>

<script
  type="application/json"
  data-section-type="product-template"
  data-section-id="{{ section.id }}"
></script>

{% if section.settings.product_breadcrumb == false %}
  {% style %}
    .breadcrumb__wrapper {
      display: none;
    }
  {% endstyle %}
{% endif %}

{% comment %}
  The contents of the product.liquid template can be found in /sections/product-template.liquid
{% endcomment %}

<section class="shopify-section-wrapper section">
  {% render 'product__structured-data' %}

  <div class="breadcrumb__wrapper one-whole column">
    <div class="breadcrumb-collection breadcrumb-collection--product">
      {% if collection.previous_product or collection.next_product %}
        <div class="nav_arrows breadcrumb_text">
          {% if collection.previous_product %}
            <a
              href="{{ collection.previous_product }}"
              title="{{ 'products.general.previous_product_html' | t }}"
              class="breadcrumb_link"
              ><span class="icon-left-arrow"> {{ 'products.general.previous_product_html' | t }}</span></a
            >
          {% endif %}
          {% if collection.previous_product and collection.next_product -%}
            <span class="breadcrumb-divider"> | </span>
          {%- endif %}

          {% if collection.next_product %}
            <a
              href="{{collection.next_product}}"
              title="{{ 'products.general.next_product_html' | t }}"
              class="breadcrumb_link"
            >
              {{- 'products.general.next_product_html' | t }}
              <span class="icon-right-arrow"></span
            ></a>
          {% endif %}
        </div>
      {% endif %}

      {% render 'product__breadcrumb' %}
    </div>
  </div>

  <div
    class="rivo-reviews-star-rating"
    data-product-id="{{ product.id }}"
    data-rating="{{ product.metafields.ba_rev.review_data.stars }}{% unless product.metafields.ba_rev.review_data.stars == 0 %}0{% endunless %}"
    data-count="{{ product.metafields.ba_rev.review_data.reviews_count }}{% unless product.metafields.ba_rev.review_data.stars != n %}0{% endunless %}"
  >
    <div class="rivo-star-wrapper"></div>
    <div class="rivo-reviews-rating-count"></div>
  </div>

  <section
    class="
      product-container
      is-flex
      is-flex-wrap
    "
  >
    {% render 'productV2',
      context: 'product',
      product: product,
      variant: product.selected_or_first_available_variant,
      gallery_thumbnails: section.settings.product_thumbs,
      gallery_click_to_zoom: section.settings.enable_product_lightbox,
      gallery_hover_zoom: section.settings.activate_zoom,
      gallery_arrows: section.settings.gallery_arrows,
      gallery_thumbnail_position: section.settings.thumbnail_position,
      gallery_thumbnail_slider: section.settings.enable_thumbnail_slider,
      gallery_slideshow_speed: section.settings.slideshow_speed,
      gallery_slideshow_transition: section.settings.slideshow_animation,
      media_position: section.settings.media_position,
      set_product_height: section.settings.set_product_height,
      product_height: section.settings.product_height,
      video_looping: section.settings.video_looping,
      show_dynamic_checkout_button: settings.show_dynamic_checkout_button,
      enable_shopify_review_comments: settings.enable_shopify_review_comments
    %}
  </section>
</section>

