                  </svg>
                  <p>Easy Returns</p>
                </div>
              </div>

            {% when 'badge' %}
              <div class="product-text">
                {% if block.settings.badge %}
                  <div class="product--image">
                    <img src="{{ '30day.svg' | asset_url }}">
                  </div>
                {% endif %}
                <div class="product--content">
                  {{ block.settings.text }}
                </div>
              </div>

